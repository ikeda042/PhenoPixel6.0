{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook --url http://127.0.0.1:6006",
    "storybook:shots": "storybook build -o storybook-static --quiet && concurrently -k -s first -n SB,TEST \"http-server storybook-static -p 6006 -s\" \"wait-on http://127.0.0.1:6006 && test-storybook --url http://127.0.0.1:6006\""
  },
  "dependencies": {
    "@chakra-ui/react": "^3.30.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/noto-sans-jp": "^5.2.8",
    "fflate": "^0.8.2",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^9.1.16",
    "@storybook/addon-interactions": "^9.1.16",
    "@storybook/react-vite": "^9.1.16",
    "@storybook/test-runner": "^0.22.0",
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "http-server": "^14.1.1",
    "playwright": "^1.50.0",
    "storybook": "^9.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.2.4",
    "wait-on": "^8.0.2"
  }
}
